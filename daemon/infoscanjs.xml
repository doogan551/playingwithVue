<service>
	<id>infoscanjs.exe</id>
	<name>InfoscanJS</name>
	<description>Infoscan web server.</description>
	<executable>C:\Program Files\nodejs\node.exe</executable>
	<logmode>rotate</logmode>
	<argument>--harmony</argument>
	<argument>C:\InfoscanJS\node_modules\node-windows\lib\wrapper.js</argument>
	<argument>--file</argument>
	<argument>C:\infoscanjs\app.js</argument>
	<argument>--log</argument>
	<argument>InfoscanJS wrapper</argument>
	<argument>--grow</argument>
	<argument>0.25</argument>
	<argument>--wait</argument>
	<argument>1</argument>
	<argument>--maxrestarts</argument>
	<argument>3</argument>
	<argument>--abortonerror</argument>
	<argument>n</argument>
	<workingdirectory>C:\InfoscanJS</workingdirectory>
</service>