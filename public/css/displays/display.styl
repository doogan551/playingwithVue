@import "nib"

//global-reset();{ revert: true }

bar = #535353
barWell = #494949
barBorder = #2a2a2a
dark = #282828
barHighlight = #696969
barShadow = #505050
spacer = #1c1c1c
textColor = #bfbfbf
inactiveText = #909090

html, body
  overflow hidden
  margin 0px
  padding 0px
  height 100%
  width 100%
  user-select none
  -moz-user-select: none;
  -webkit-user-select: none;
  /* this will work for QtWebKit in future */
  -webkit-user-drag: none;

#container

  height 100%
  width 100%
  position relative
  //background-color red

.grid1
  background-image url('/images/gridy5.png'), url('/images/gridx5.png')
  position absolute
  top 0px
  left 0px
  background-position: 0px 0px, 0px 0px;
  //background-repeat: repeat-y;
  width 100%
  height 100%
  display none

.grid2
  background-image url('/images/gridy5.png'), url('/images/gridx5.png')
  position absolute
  top 0px
  left 0px
  background-position: 0px 0px, 0px 0px;
  //background-repeat: repeat-y;
  width 100%
  height 100%
  display none

#display
  position absolute
  top 50%
  left 50%
  -moz-user-select none
  -webkit-user-select none
  user-select none

.bg
  position absolute
  width 100%
  height 100%
  background-repeat no-repeat

div.object
  position absolute

.glow .highlight
  background-color rgba(255,255,255,.7)
  position absolute
  top 0px;
  left 0px
  height 100%
  width 100%
  border-radius 3px
  box-shadow inset 0 0 2px #000000;
  //border 1px solid #000000

.displayButton
  cursor pointer
.closeTab
  background-image url('/images/barClose.png')
  background-repeat no-repeat
  background-position 10px 8px

.minBar
  width 33px
  height 100%
  position absolute
  left 0px
  top 0px
  display none
  background-image url('/images/minBg.png')

.leftToolbar
  display none
  left -220px

.rightToolbar
  position absolute
  top 0px
  right -220px


.displayButton, .controlButton
  padding 0px 0px 0px 0px
  padding-top 0px

  cursor pointer
  display block
  text-align center
  text-decoration none
  margin 0px
  background linear-gradient(top, #ccc, #888)
  filter unquote("progid:DXImageTransform.Microsoft.gradient(startColorstr='#cccccc', endColorstr='#888888',GradientType=0)")
  color #222
  //text-shadow 0 1px 0 rgba(255,255,255,30%)//
  border-radius 3px
  box-shadow inset 0 0 2px #000000;
  //border 1px solid rgba(0,0,0,75%)
  //box-shadow 0 -1px 0px 0 rgba(0,0,0,50%) inset, 0 1px 1px 0 rgba(255,255,255,50%) inset

.displayButton:hover, .controlButton:hover
  box-shadow inset 0 0 2px #ffffff;

#display .trans
  background none
  border 1px solid transparent
  box-shadow none

#display .trans:hover
  border 1px solid rgba(0,0,0,75%)
  background-color rgba(255,255,255,10%)
#zoverlay
  position absolute
  top 0px
  left 0px
  width 100%
  height 100%
  //background-color rgba(255,255,255,.5)
  z-index 8000
  display none
body .pop
  top -10px
  left 0px
body .pop .bgg
  background-image url('/images/popBg.png')
  padding-left 33px
  padding-right 30px
  color #909090
  cursor pointer
  font-size 14px
  line-height 18px
  display block
  position relative

.version
  position relative

body .pop .bgg a
  color #909090
  text-decoration none
  position absolute
  top 0px
  right 2px
#menu
  color #909090
  text-decoration none
  position absolute
  top 0px
  right 2px
  width 235px
  display none
#menu .bgg

  background-image url('/images/popBg.png')
  padding-left 33px
  padding-right 30px
  color #909090
  cursor pointer
  font-size 14px
  line-height 18px
  display block
  position relative
#menu .bgg a
  display block
  text-align center
  color white
  text-decoration none
#menu2
  color #909090
  text-decoration none
  position absolute
  top 0px
  right 2px
  width 235px
  display none
#menu2 .bgg

  background-image url('/images/popBg.png')
  padding-left 33px
  padding-right 30px
  color #909090
  cursor pointer
  font-size 14px
  line-height 18px
  display block
  position relative
#menu2 .bgg a
  display block
  text-align center
  color white
  text-decoration none
.paste
  display none
.clipBoard div
  font-size 10px
  color #bfbfbf
  padding-top 5px
  padding-bottom 5px
.selectiondiv
  position absolute
  border:1px dotted white;
  background:#1B94E0;
  opacity:0.4;
  //filter:alpha(opacity=40);
  margin:0px;
  padding:0px;
  display:none;
#displayClip
  position absolute