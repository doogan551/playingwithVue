{
    // Use IntelliSense to learn about possible Node.js debug attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Import",
            "program": "${workspaceRoot}/apps/newimport.js",
            "cwd": "${workspaceRoot}"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "buildTree",
            "program": "${workspaceRoot}/apps/buildTreeFromPoints.js",
            "cwd": "${workspaceRoot}"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "backupHistory",
            "program": "${workspaceRoot}/apps/backupHistory.js",
            "cwd": "${workspaceRoot}"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "rectest",
            "program": "${workspaceRoot}/apps/rectest.js",
            "cwd": "${workspaceRoot}"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Migration",
            "program": "${workspaceRoot}/apps/migrationScripts.js",
            "cwd": "${workspaceRoot}"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Server",
            "program": "${workspaceRoot}\\app.js",
            "cwd": "${workspaceRoot}"
        },
        {
            "type": "node",
            "request": "attach",
            "name": "Attach to Process",
            "port": 5858
        },
        {
            "name": "Run tests",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/node_modules/mocha/bin/_mocha",
            "stopOnEntry": false,
            "args": ["tests", "--no-timeouts"],
            "cwd": "${workspaceRoot}",
            "runtimeExecutable": null
        },
        {
            "name": "Run import tests",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/node_modules/mocha/bin/_mocha",
            "stopOnEntry": false,
            "args": ["tests/import", "--no-timeouts"],
            "cwd": "${workspaceRoot}",
            "runtimeExecutable": null
        }
    ]
}

            