							.ui-bar.ui-bar-c Required
						.ui-block-d
							.ui-bar.ui-bar-c Array of Objects
					#rows.ui-grid-c
						.ui-block-a
							.ui-bar.ui-bar-c Month
						.ui-block-b
							.ui-bar.ui-bar-c Month of calendar entry
						.ui-block-c
							.ui-bar.ui-bar-c Required for each calendar entry
						.ui-block-d
							.ui-bar.ui-bar-c String
					#rows.ui-grid-c
						.ui-block-a
							.ui-bar.ui-bar-c Day
						.ui-block-b
							.ui-bar.ui-bar-c Day of calendar entry
						.ui-block-c
							.ui-bar.ui-bar-c Required for each calendar entry
						.ui-block-d
							.ui-bar.ui-bar-c Integer
					#rows.ui-grid-c
						.ui-block-a
							.ui-bar.ui-bar-c Year
						.ui-block-b
							.ui-bar.ui-bar-c Year of calendar entry
						.ui-block-c
							.ui-bar.ui-bar-c Required for each calendar entry
						.ui-block-d
							.ui-bar.ui-bar-c Integer
					#rows.ui-grid-c
						.ui-block-a
							.ui-bar.ui-bar-c Comment
