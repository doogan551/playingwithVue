doctype html
html
    head
        title
            | Infoscan by Dorsett Technologies
        link(rel='stylesheet', href='/css/lib/materialize.css')
        link(rel='stylesheet', href='/sass/v2/materialize')
        link(rel='stylesheet', href='/css/lib/font-awesome.4.6.1.css')
        //- link(rel='stylesheet', href='/css/lib/jquery-ui.css')


    body
        ul#dropdown1.dropdown-content
            li
                a.blue-grey-text.truncate.tooltipped(href='javascript://', data-position='right', data-tooltip='4194 DXAC DISP', data-delay='10') 4194 DXAC DISP
            li
                a.blue-grey-text.truncate.tooltipped(href='javascript://', data-position='right', data-tooltip='4200 AH1 DISP', data-delay='10') 4200 AH1 DISP
            //- li.divider
            li
                a.blue-grey-text.truncate.tooltipped(href='javascript://', data-position='right', data-tooltip='4250 FLR DISP', data-delay='10') 4250 FLR DISP

        header
            .navbar-fixed.taskbar
                nav.blue-grey
                    .nav-wrapper
                        ul.left
                            li
                                a#startButton.noHover.tooltipped(href="javascript://", data-position='bottom', data-tooltip='New Window')
                                    img.circle.dorsett-logo(src='/img/Dorsett_icon.png')
                            li.active
                                a.dropdown-button(href='javascript://', data-activates='dropdown1', data-beloworigin='true', data-hover='true')
                                    | Displays
                                    i.material-icons.right arrow_drop_down
                            li.active
                                a.waves-effect.truncate.tooltipped(href='javascript://', data-position='bottom', data-tooltip='4200 PARKING LOT LIGHTS', data-delay='10') 4200 PARKING LOT LIGHTS
                        ul.right.hide-on-med-and-down
                            li
                                form
                                    .input-field
                                        input#search(type='search', required='')
                                        label(for='search')
                                            i.material-icons search
                                        i.material-icons close
                            //- li
                            //-     span.clock 10:10

        main
            #modal1.modal
                .modal-content
                    h4 Modal Header
                    p A bunch of text
                .modal-footer
                    a.modal-action.modal-close.waves-effect.btn-flat(href='javascript://') Close

                    //- .row
                    //-     .col.m6
                    //- .card-panel.blue-grey.z-depth-2
                    //-     span.white-text This is a simple panel

            .card-panel.dti-card-panel.blue-grey.z-depth-2.lighten-3(style="left: 5px; top: 55px;")
                .card-toolbar
                    .card-title.center-align.white-text
                        div 4194 DXAC DISP
                    a.white-text(href="javascript://")
                        i.material-icons skip_previous
                    a.white-text(href="javascript://")
                        i.material-icons skip_next
                    a.right.white-text(href="javascript://")
                        i.material-icons close
                img(src="/img/test/display.png", style="height: 100%; width: 100%;")

            .card-panel.dti-card-panel.blue-grey.z-depth-2.activeCard(style="left: 55px; top: 105px;")
                .card-toolbar
                    .card-title.center-align.white-text
                        div 4200 PARKING LOT LIGHTS
                    a.white-text(href="javascript://")
                        i.material-icons skip_previous
                    a.white-text(href="javascript://")
                        i.material-icons skip_next
                    a.right.white-text(href="javascript://")
                        i.material-icons close
                img(src="/img/test/sequence.png", style="height: 100%; width: 100%;")

            #newPanel.card-panel.dti-card-panel.blue-grey.z-depth-2.hide(style="left: 110px; top: 155px;")
                .card-toolbar
                    .card-title.center-align.white-text
                        div New Window
                    a.white-text(href="javascript://")
                        i.material-icons skip_previous
                    a.white-text(href="javascript://")
                        i.material-icons skip_next
                    a.right.white-text(href="javascript://")
                        i.material-icons close
                img(src="/img/test/newwindow.png", style="height: 100%; width: 100%;")

                //- .card.blue-grey.z-depth-2
                //-     .card-content.white-text
                //-         span.card-title Card Title
                //-         p
                //-             | Card Content
                //-     .card-action.card-action-right
                //-         a(href='javascript://') Cancel
                //-         a(href='javascript://') Confirm
        footer

    script(type='text/javascript', src='/js/lib/jquery-1.9.1.min.js')
    script(type='text/javascript', src='/js/lib/jquery-ui.js')
    //- script(type='text/javascript', src='/js/lib/config.js')
    script(type='text/javascript', src='/js/lib/moment.js')
    //- script(type='text/javascript', src='/js/lib/knockout-3.2.0.js')
    //- script(type='text/javascript', src='/js/lib/socket.io.js')
    script(type='text/javascript', src='/js/lib/materialize.js')
    script(type='text/javascript', src='/js/v2/home.js')
